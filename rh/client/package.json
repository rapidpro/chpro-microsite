{
  "name": "rapidpro_for_health",
  "version": "0.1.0",
  "description": "RapidPro for Health site built with Django CMS",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "yarn run build",
    "parcel": "parcel",
    "build": "yarn run build_site",
    "watch": "yarn run watch_site",
    "build_site": "parcel build --public-url=/static/ ./all.js",
    "build_admin": "parcel build --public-url=/static/ ./admin.js",
    "watch_site": "parcel watch --public-url=/static/ ./all.js",
    "watch_admin": "parcel watch --public-url=/static/ ./admin.js",
    "build_guide": "./node_modules/.bin/kss --config kss-config.json; node-sass static/styleguide/kss-assets/kss.scss static/styleguide/kss-assets/kss.css"
  },
  "engines": {
    "yarn": "^1.4.x"
  },
  "dependencies": {
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.14.0",
    "foundation-sites": "^6.4.4-rc1",
    "jquery": "^3.2.1",
    "node-sass": "^4.13.1",
    "parcel-bundler": "^1.5.1",
    "post-load-plugins": "^0.0.16",
    "select2": "^4.0.6-rc.1",
    "autoprefixer": "^8.2.0",
    "postcss-modules": "^1.1.0",
    "svg-sprite": "^1.4.0",
    "yarn": "^1.4.x"
  },
  "devDependencies": {
    "kss": "^3.0.0-beta.18"
  },
  "license": "UNLICENSED"
}
